fun main() {
    println("Loops Example")
    var count = 1
   while (count <= 10)
   {
//        body statements
       println("durgesh $count")
       count++
   }

   println("Loop exited")

   do {
       println("durgesh $count")
       count++
   } while (count <= 10);

   for (i in 100..110)
   {

       println("For loop $i")

   }
}
